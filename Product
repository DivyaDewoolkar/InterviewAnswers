##1. Creating table Product 
CREATE TABLE product(       
p_id integer,       
p_price integer,       
p_name varchar(20),       
p_warranty integer,       
p_qty integer     
); 

insert into product values(1,35000,'Washing Machine',3,10); 
insert into product values(2,45000,'Smart TV',2,5); 
insert into product values(3,33000,'AC',5,6); 
insert into product values(4,20000,'Home Theatre',1,0);  

